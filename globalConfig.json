{
    "pages": {
        "configuration": {
            "tabs": [
                {
                    "name": "proxy",
                    "entity": [
                        {
                            "type": "checkbox",
                            "label": "Enable",
                            "field": "proxy_enabled"
                        },
                        {
                            "type": "singleSelect",
                            "label": "Proxy Type",
                            "options": {
                                "disableSearch": true,
                                "autoCompleteFields": [
                                    {
                                        "value": "http",
                                        "label": "http"
                                    },
                                    {
                                        "value": "socks4",
                                        "label": "socks4"
                                    },
                                    {
                                        "value": "socks5",
                                        "label": "socks5"
                                    }
                                ]
                            },
                            "defaultValue": "http",
                            "field": "proxy_type"
                        },
                        {
                            "type": "text",
                            "label": "Host",
                            "validators": [
                                {
                                    "type": "string",
                                    "errorMsg": "Max host length is 4096",
                                    "minLength": 0,
                                    "maxLength": 4096
                                }
                            ],
                            "field": "proxy_url"
                        },
                        {
                            "type": "text",
                            "label": "Port",
                            "validators": [
                                {
                                    "type": "number",
                                    "range": [
                                        1,
                                        65535
                                    ]
                                }
                            ],
                            "field": "proxy_port"
                        },
                        {
                            "type": "text",
                            "label": "Username",
                            "validators": [
                                {
                                    "type": "string",
                                    "errorMsg": "Max length of username is 50",
                                    "minLength": 0,
                                    "maxLength": 50
                                }
                            ],
                            "field": "proxy_username"
                        },
                        {
                            "type": "text",
                            "label": "Password",
                            "validators": [
                                {
                                    "type": "string",
                                    "errorMsg": "Max length of password is 8192",
                                    "minLength": 0,
                                    "maxLength": 8192
                                }
                            ],
                            "encrypted": true,
                            "field": "proxy_password"
                        },
                        {
                            "type": "checkbox",
                            "label": "Reverse DNS resolution",
                            "field": "proxy_rdns"
                        }
                    ],
                    "options": {
                        "saveValidator": "function(formData) { if(!formData.proxy_enabled || formData.proxy_enabled === '0') {return true; } if(!formData.proxy_url) { return 'Proxy Host can not be empty'; } if(!formData.proxy_port) { return 'Proxy Port can not be empty'; } return true; }"
                    },
                    "title": "Proxy"
                },
                {
                    "type": "loggingTab"
                },
                {
                    "name": "additional_parameters",
                    "title": "Teams Add-on configuration",
                    "entity": [
                        {
                            "field": "default_ms_teams_url",
                            "label": "Default MS team channel:",
                            "type": "text",
                            "help": "Webhook URL.\n(https enforced, can be overridden on a per alert basis)",
                            "required": false,
                            "defaultValue": "",
                            "validators": [
                                {
                                    "type": "string",
                                    "minLength": 0,
                                    "maxLength": 8192,
                                    "errorMsg": "Max length of text input is 8192"
                                }
                            ]
                        },
                        {
                            "field": "default_ms_teams_image_link",
                            "label": "Default MS teams image link",
                            "type": "text",
                            "help": "Picture URL\n(can be overridden on a per alert basis)",
                            "required": false,
                            "defaultValue": "",
                            "validators": [
                                {
                                    "type": "string",
                                    "minLength": 0,
                                    "maxLength": 8192,
                                    "errorMsg": "Max length of text input is 8192"
                                }
                            ]
                        },
                        {
                            "field": "default_ms_teams_check_url_compliancy",
                            "label": "URL regex compliancy checker",
                            "type": "text",
                            "help": "You can define a regular expression used to verify that URL is compliant with your rules",
                            "required": false,
                            "defaultValue": ".*",
                            "validators": [
                                {
                                    "type": "string",
                                    "minLength": 0,
                                    "maxLength": 8192,
                                    "errorMsg": "Max length of text input is 8192"
                                }
                            ]
                        }
                    ]
                }
            ],
            "title": "Configuration",
            "description": "Set up your add-on"
        }
    },
    "alerts": [
